---
export interface Props {
  cta: string;
}

const { cta } = Astro.props;
---

<button class="rounded-full px-8 py-3 w-full">
  {cta}
</button>

<style>
  button {
    color: #ffffff;
    background-image: linear-gradient(170deg, #4cb8d5, #3382cc);
    font-weight: 700;
    font-family: "Raleway", sans-serif;
    font-size: 14px;
  }
</style>
